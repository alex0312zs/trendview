/*每个页面公共css */

@charset "UTF-8";

@font-face{
     font-family: 'Kanit'; 
     src: url(/static/font/Kanit-Regular.ttf) format('truetype');
}

/* CSS Document */

.period_head{
	height: 0.7rem;
	display: flex;
	justify-content: space-around;
	padding: .1rem;
	/* background-color: #007AFF; */
}

.activeList{
	font-weight: 500;
	height: .3rem;
	padding: .1rem;
	border-bottom: 1rpx solid #4495F3;
}

.normal_list{
	width: 50%;
	text-align: center;
	/* margin: 0.1rem; */
}

.gray_line{
	height: 0.14rem;
	background-color: #eef2f5;
}

.bgblue{
	background-image: url(data:image/png;base64,);
	background-repeat: no-repeat;
	background-size: 100%;
	border-radius: 50%;
	width: 0.3rem;
	height: 0.3rem;
	line-height: 0.3rem;
	color: #FFFFFF !important;
	font-size: 0.24rem;
	position: relative;
	vertical-align: middle;
}

.bgred{
	background-image: url(data:image/png;base64,);
	background-repeat: no-repeat;
	background-position:center;
	background-size: 0.4rem 0.4rem;
	line-height: 0.5rem;
	color: #FFFFFF !important;
	font-size: 0.24rem;
	position: relative;
	vertical-align: middle;
}

page {
	-webkit-overflow-scrolling: touch;
	position: fixed;
	left: 0;
	top: 0;
	font-size: .3rem !important;
	/* font-family: "Microsoft YaHei", PingFangSC-Regular, Arial; Kanit Regular */
	font-family: "Kanit";  
	color: #181818;
	background: #fafafa;
	overflow-x: hidden;
}

page,
page {
	width: 100vw;
	height: 100vh;
}

page {
	font-size: 13.3333333vw !important;
}

.period {
	padding: 0.1rem 0.18rem;
	width: 100%;
	z-index: 99;
}

.front_num{
	background: #ecf1f7;
	border-radius: 3rpx;
	width: 2rem;
	height: .5rem;
	line-height: .5rem;
	text-align: center;
}

.qxc-flex {
	position: absolute;
	top: 2.3rem;
	bottom: 0;
	left: 0;
	right: 0;
	display: flex;
}

.qxc-left {
	height: 100%;
	display: flex;
	flex-direction: column;
	width: 2.93rem;
	overflow: hidden;
	position: relative;
}

.qxc-right {
	width: 87.6%;
	overflow: hidden;
	height: 100%;
	display: flex;
	flex-direction: column;
}

._table {
	border-collapse: collapse;
	width: 100%;
	text-align: center;
}

.l-top {
	z-index: 2;
	height: 0.9rem;
}

.l-top ._th {
	height: 0.86rem;
	font-size: 0.19rem;
	color: #353A47;
	background-color: #e5e8ee;
	font-weight: normal;
	border-right: 1rpx solid #FFFFFF;
	/*no*/
	border-bottom: 1rpx solid #FFFFFF;
	/*no*/
}

