}

.r-bot {
	overflow: hidden;
	width: 100%;
	position: relative;
	top: -0.01rem;
	flex: 1;
	height: auto;
}

.fixedhead {
	position: fixed;
}

.r-fd {
	position: absolute;
	top: 0;
	left: 0;
	right: 0;
	bottom: 0;
}

.r-top {
	height: 0.9rem;
	z-index: 1;
}

.r-top ._table,.r-bot ._table{
	table-layout: fixed;
	width:30rem;
}

